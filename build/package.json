{
  "name": "build",
  "version": "1.0.0",
  "description": "Video to ASCII converter",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "pkg index.js --target node16-win-x64 --output dist/videotoascii.exe",
    "devpkg": "npm install pkg --save-dev"
  },
  "pkg": {
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "dist"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}